#include <stdio.h>

class Matrix{
	private:
	int row;
	int col;
	int mat[10][10];

	public:
	void input_mat();
};

void Matrix :: input_mat()
{
	printf("\nEnter No. of Rows : ");
	scanf("%d",&row);
	printf("\nEnter No. of Columns : ");
	scanf("%d",&col);

	int i, j;
	for(i=0; i<row; ++i)
	{
		for(j=0; j<col; ++j)
		{
			printf("\nEnter Element (%d,%d) : ",i+1,j+1);
			scanf("%d",&mat[i][j]);
		}
	}
}

void main()
{
	Matrix obj;
	obj.input_mat();
}
