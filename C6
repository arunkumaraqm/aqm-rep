#include <stdio.h>

void input(int *number)
{
    printf("\nEnter Number : ");
    scanf("%d",number);
    if(*number<0)
    {
        *number=-*number;
    }
}
int chk_palindrome(int number)
{
    if(number<11) return -1;
    
    int arr[5], i;
    for(i=0; number!=0; ++i)
    {
        arr[i]=number%10;
        number=number/10;
    }
    
    int temp=i/2,noofdigits=i;
    
    for(i=0; i<temp; ++i)
    {
        if(arr[i]!=arr[noofdigits-i-1])
        return -1;
    }
    return 1;
}

void output(int flag)
{
    (flag==-1)? printf("\nNot a palindrome") : printf("\nIt is a palindrome");
}
void main()
{
    int number;
    input(&number);
    output(chk_palindrome(number));
}
